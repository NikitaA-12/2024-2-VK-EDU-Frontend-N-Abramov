{
  "extends": "standard",
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "18.3"
    }
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
